<mujoco model="ur3e">
    <compiler angle="radian" meshdir="ur3e/collision/" texturedir="ur3e/visual/" />
    <option collision="all" />

    <default class="UR3e">
        <joint damping='45'/>
    </default>
    </default>
        <camera mode="fixed" pos="0 0 3"/>
    </default>

    <asset>
        <mesh name="base"     file="base.stl" />
        <mesh name="shoulder" file="shoulder.stl" />
        <mesh name="upperarm" file="upperarm.stl" />
        <mesh name="forearm"  file="forearm.stl" />
        <mesh name="wrist1"   file="wrist1.stl" />
        <mesh name="wrist2"   file="wrist2.stl" />
        <mesh name="wrist3"   file="wrist3.stl" />

        <texture name="floor_tex"   type="2d"        builtin="checker" rgb1=".2 .3 .4" rgb2=".1 .2 .3" width="100" height="100" />
        <material name='floor_mat'      texture="floor_tex"  shininess="0.8" texrepeat="10 10" specular="0.7"  reflectance="0.5" />
        <texture name="texdenim" type="2d" file="denim.png"/>
        <material name="matdenim" texture="texdenim"/>

    </asset>

    <worldbody>
        <light name="lightdefault" diffuse=".3 .3 .3" pos="0 0 3" dir="0 0 -1" />

         <geom name='floor' material="floor_mat" pos='0 0 0' size='2.5 2.5 0.1'  type='plane' />
         <!-- table  -->
        <body name="table" pos="0 0 0">
            <geom type="box" size="0.5 .5 .1" rgba="1 1 1 1"/>
            <body name="base" pos="0 0 .1">
                <!-- <geom type="mesh" contype="0" conaffinity="0" group="1" rgba="0.7 0.7 0.7 1" mesh="base" /> -->
                <geom type="mesh" rgba="0.7 0.7 0.7 1" mesh="base" />
                <body name="shoulder_link" pos="0 0 0.152">
                    <inertial pos="0 0 0" mass="2" diaginertia="0.00809316 0.00809316 0.005625" />
                    <joint name="shoulder_pan_joint" class="UR3e" pos="0 0 0" axis="0 0 1" limited="true" range="-5.75 5.75" />
                    <!-- <geom type="mesh" contype="0" conaffinity="0" group="1" rgba="0.7 0.7 0.7 1" mesh="shoulder" /> -->
                    <geom type="mesh" rgba="0.7 0.7 0.7 1" mesh="shoulder" />
                    <body name="upper_arm_link" pos="0 0.12 0" quat="0.707107 0 0.707107 0">
                        <inertial pos="0 0 0.122" mass="3.42" diaginertia="0.0217771 0.0217771 0.00961875" />
                        <joint name="shoulder_lift_joint" class="UR3e" pos="0 0 0" axis="0 1 0" limited="true" range="-5.75 5.75" />
                        <!-- <geom type="mesh" contype="0" conaffinity="0" group="1" rgba="0.7 0.7 0.7 1" mesh="upperarm" /> -->
                        <geom type="mesh" rgba="0.7 0.7 0.7 1" mesh="upperarm" />
                        <body name="forearm_link" pos="0 -0.093 0.244">
                            <inertial pos="0 0 0.1065" mass="1.26" diaginertia="0.00653562 0.00653562 0.00354375" />
                            <joint name="elbow_joint" class="UR3e" pos="0 0 0" axis="0 1 0" limited="true" range="-2.618 2.618" />
                            <!-- <geom type="mesh" contype="0" conaffinity="0" group="1" rgba="0.7 0.7 0.7 1" mesh="forearm" /> -->
                            <geom type="mesh" rgba="0.7 0.7 0.7 1" mesh="forearm" />
                            <body name="wrist_1_link" pos="0 0 0.213" quat="0.707107 0 0.707107 0">
                                <inertial pos="0 0 0" quat="0.5 0.5 -0.5 0.5" mass="0.8" diaginertia="0.00225 0.002085 0.002085" />
                                <joint name="wrist_1_joint" class="UR3e" pos="0 0 0" axis="0 1 0" limited="true" range="-0.9425 0.9425" />
                                <!-- <geom type="mesh" contype="0" conaffinity="0" group="1" rgba="0.7 0.7 0.7 1" mesh="wrist1" /> -->
                                <geom type="mesh" rgba="0.7 0.7 0.7 1" mesh="wrist1" />
                                <body name="wrist_2_link" pos="0 0.104 0">
                                    <inertial pos="0 0 0" quat="0.5 0.5 -0.5 0.5" mass="0.8" diaginertia="0.00225 0.002085 0.002085" />
                                    <joint name="wrist_2_joint" class="UR3e" pos="0 0 0" axis="0 0 1" limited="true" range="-0.9425 0.9425" />
                                    <!-- <geom type="mesh" contype="0" conaffinity="0" group="1" rgba="0.7 0.7 0.7 1" mesh="wrist2" /> -->
                                    <geom type="mesh" rgba="0.7 0.7 0.7 1" mesh="wrist2" />
                                    <body name="wrist_3_link" pos="0 0 0.085">
                                        <inertial pos="0 0.000233618 0" quat="0.5 0.5 -0.5 0.5" mass="0.351" diaginertia="0.000991097 0.000918909 0.000912204" />
                                        <joint name="wrist_3_joint" class="UR3e" pos="0 0 0" axis="0 1 0" limited="true" range="-0.9425 0.9425" />
                                        <!-- <geom type="mesh" contype="0" conaffinity="0" group="1" rgba="0.7 0.7 0.7 1" mesh="wrist3" /> -->
                                        <geom type="mesh" rgba="0.7 0.7 0.7 1" mesh="wrist3" />
                                        <geom size="0.005 0.005 0.005" pos="0 0.082 0" quat="0.707107 0 0 0.707107" type="box" />
                                    </body>
                                </body>
                            </body>
                        </body>
                    </body>
                </body>
            </body>
        </body>
        
        <!-- cloth -->
        <body name="B3_5" pos="0.25 0.25 0.1">
            <freejoint/>
            <composite type="cloth" count="6 6" spacing="0.05" flatinertia="0.01">
                <joint kind="main" damping="0.001"/>
                <skin material="matdenim" texcoord="true" inflate="0.005" subgrid="2"/>
                <geom type="capsule" size="0.0015 0.001" rgba=".8 .2 .1 1"/>
            </composite>
        </body>
        
    </worldbody>

    <!-- Actuators -->
    <actuator>
      <motor name="shoulder_pan_motor"  joint="shoulder_pan_joint"  ctrllimited="true" ctrlrange="-5.75 5.75" /> 
      <motor name="shoulder_lift_motor" joint="shoulder_lift_joint" ctrllimited="true" ctrlrange="-5.75 5.75" /> 
      <motor name="elbow_motor"         joint="elbow_joint"         ctrllimited="true" ctrlrange="-2.618 2.618" /> 
      <motor name="wrist_1_motor"       joint="wrist_1_joint"       ctrllimited="true" ctrlrange="-0.9425 0.9425" /> 
      <motor name="wrist_2_motor"       joint="wrist_2_joint"       ctrllimited="true" ctrlrange="-0.9425 0.9425" /> 
      <motor name="wrist_3_motor"       joint="wrist_3_joint"       ctrllimited="true" ctrlrange="-0.9425 0.9425" /> 
    </actuator>

    <!-- <camera>
    <camera name="realsense" pos="10 10 10" quat="0 0 1 0" />
    <image width="640" height="480" hfov="1.5708" format="RGB8" near="0.01" far="50.0"/>
    </camera> -->

</mujoco>