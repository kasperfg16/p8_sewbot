<mujoco model="ur5">
    <compiler angle="radian" meshdir="ur5" balanceinertia="true"/>
    <option collision="all" />


    <default>
        <light castshadow="false" diffuse="1 1 1" />
        <camera fovy="60" />
    </default>
    <default class="UR5">
        <joint damping='45'/>
    </default>

